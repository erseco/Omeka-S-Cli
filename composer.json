{
    "name": "ghentcdh/omeka-s-cli",
    "description": "Omeka-S Command Line Tool",
    "type": "project",
    "version": "0.5.0",
    "keywords": ["Omeka S"],
    "homepage": "https://github.com/GhentCDH/Omeka-S-Cli",
    "authors": [
        {
            "name": "Frederic Lamsens",
            "email": "frederic.lamsens@ugent.be"
        }
    ],
    "license": "MIT",
    "require": {
        "ext-zip": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "adhocore/cli": "^v1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/console": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "OSC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omeka\\": "tests/Stubs/Omeka/",
            "Laminas\\": "tests/Stubs/Laminas/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    }
}
